{
  "name": "patchcab-kaleido",
  "description": "Patchcab kaleidoscope style video module",
  "version": "1.0.2",
  "author": {
    "name": "Spectrome",
    "url": "https://github.com/spectrome"
  },
  "license": "MIT",
  "files": [
    "modules"
  ],
  "scripts": {
    "start": "patchcab",
    "build": "patchcab --build",
    "prepublish": "yarn build"
  },
  "patchcab": [
    {
      "name": "Kaleido",
      "file": "Kaleido",
      "size": {
        "w": 18,
        "h": 1
      },
      "tags": [
        "video"
      ],
      "libs": [
        "Tone",
        "THREE"
      ]
    }
  ],
  "devDependencies": {
    "@patchcab/core": "1.0.0",
    "@patchcab/modules": "1.0.0",
    "three": "^0.124.0"
  },
  "prettier": {
    "useTabs": false,
    "arrowParens": "always",
    "semi": true,
    "bracketSpacing": true,
    "singleQuote": true,
    "printWidth": 120,
    "svelteSortOrder": "options-scripts-styles-markup"
  }
}
